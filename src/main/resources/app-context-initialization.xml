<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:c="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
        default-lazy-init="true"
>

    <bean id="InitializingBean" class="com.spring.gameloft.lifecycle.ProgrammerInitializingBean"
          p:age="25"
          p:name="Andrei"
          lazy-init="default"
    />
    <bean id="InitializingBean1" class="com.spring.gameloft.lifecycle.ProgrammerInitializingBean"
          p:age="27"
          p:name="Andrei"
    />
    <bean id="InitializingBean2" class="com.spring.gameloft.lifecycle.ProgrammerInitializingBean"
          p:age="25"
    />
</beans>